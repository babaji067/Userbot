{
  "name": "telegram-userbot-auto-message",
  "description": "Userbot to send auto message in all joined groups",
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID",
      "required": true
    },
    "API_HASH": {
      "description": "Your Telegram API Hash",
      "required": true
    },
    "SESSION_STRING": {
      "description": "Pyrogram Session String of your account",
      "required": true
    },
    "CONTROL_GROUP_ID": {
      "description": "Control Group ID (e.g. -1001234567890)",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}
