{
  "name": "TelegramUserbot",
  "description": "Userbot for auto messaging with Heroku",
  "env": {
    "API_ID": {
      "description": "Telegram API ID",
      "required": true
    },
    "API_HASH": {
      "description": "Telegram API HASH",
      "required": true
    },
    "SESSION_STRING": {
      "description": "Pyrogram Session String",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}
